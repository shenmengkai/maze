{
  "name": "maze",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "node --test --test-reporter spec",
    "prezip": "mkdir -p deploy",
    "zip": "find . -maxdepth 2 -name '*.mjs' ! -name '*.test.mjs' -exec zip -j deploy/function.zip {} package.json \\;",
    "predeploy": "npm run zip",
    "deploy": "aws lambda update-function-code --function-name genMaze --zip-file fileb://deploy/function.zip"
  }
}
